{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@now/python"
        },
        {
            "src": "/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}